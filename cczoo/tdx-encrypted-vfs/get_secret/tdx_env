export LC_ALL=C.UTF-8 LANG=C.UTF-8

export INSTALL_PREFIX=/usr/local
export C_INCLUDE_PATH=/opt/intel/sgxsdk/include
export CPLUS_INCLUDE_PATH=${C_INCLUDE_PATH}:${CPLUS_INCLUDE_PATH}
export LD_LIBRARY_PATH=${INSTALL_PREFIX}/lib:${INSTALL_PREFIX}/lib/x86_64-linux-gnu:/opt/intel/sgxsdk/lib64:${LD_LIBRARY_PATH}
export PATH=${INSTALL_PREFIX}/bin:${LD_LIBRARY_PATH}:${CPLUS_INCLUDE_PATH}${PATH}

export GRPC_ROOT=/tmp
export GRPC_PATH=${GRPC_ROOT}/grpc
export GRPC_VERSION=v1.38.1

export SGX_RA_TLS_BACKEND=TDX